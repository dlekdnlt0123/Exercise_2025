    {
      "cell_type": "code",
      "source": [
        "class Add:\n",
        "    def add(self, n1, n2):\n",
        "        return n1+n2\n",
        "\n",
        "class Multiply:\n",
        "    def multiply(self, n1, n2):\n",
        "        return n1*n2\n",
        "\n",
        "class Calculator(Add, Multiply):\n",
        "    def sub(self, n1, n2):\n",
        "        return n1-n2\n",
        "\n",
        "\n",
        "obj = Calculator()\n",
        "print(obj.add(1, 2))\n",
        "print(obj.multiply(3, 2))"
      ],
      "metadata": {
        "id": "sZ1lZmlkKt0Y",
