        "class Add:\n",
        "    def add(self, n1, n2):\n",
        "        return n1+n2\n",
        "\n",
        "class Calculator(Add):\n",
        "    def sub(self, n1, n2):\n",
        "        return n1-n2\n",
        "\n",
